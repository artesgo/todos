{
	"name": "Artesgo Todos",
	"description": "Groupable todo list, no signin or connection required",
	"version": "2.1",
	"manifest_version": 3,
	"permissions": ["storage"],
	"action": {
		"default_popup": "./index.html"
	},
	"icons": {
		"16": "icon/arrow-down--16.png",
		"48": "icon/arrow-down--48.png",
		"128": "icon/arrow-down--128.png"
	}
}
